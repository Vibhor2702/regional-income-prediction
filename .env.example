# Census API Configuration
# Get your Census API key from: https://api.census.gov/data/key_signup.html
CENSUS_API_KEY=your_census_api_key_here

# IRS Data URLs (update these with current year URLs)
IRS_ZIP_DATA_URL=https://www.irs.gov/pub/irs-soi/zipcode.zip
IRS_COUNTY_DATA_URL=https://www.irs.gov/pub/irs-soi/countydata.zip

# HUD Crosswalk URL
HUD_ZIP_COUNTY_CROSSWALK_URL=https://www.huduser.gov/portal/datasets/usps_crosswalk.html

# TIGER/Line Shapefiles Base URL
TIGER_SHAPEFILE_BASE_URL=https://www2.census.gov/geo/tiger/TIGER2023

# Model Configuration
RANDOM_SEED=42
TEST_SIZE=0.2
CV_FOLDS=5

# Optuna Configuration
N_TRIALS=100
OPTUNA_TIMEOUT=3600

# Streamlit Configuration
STREAMLIT_PORT=8501
STREAMLIT_SERVER_HEADLESS=true
